{
  "expo": {
    "name": "Leazzy",
    "slug": "Leazzy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "leazzy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.leazzy.app",
      "config": {
        "googleMapsApiKey": "AIzaSyDuT_dlx3fUfPhxSRqFxOZTFHFWWXU0IZI"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Leazzy needs your location to find properties near you and to mark your property location.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.leazzy.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDuT_dlx3fUfPhxSRqFxOZTFHFWWXU0IZI"
        }
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-location",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          },
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 33,
            "buildToolsVersion": "35.0.0",
            "enableProguardInReleaseBuilds": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8472a417-2fa7-4c6d-91a9-b08302f97d70"
      }
    }
  }
}
